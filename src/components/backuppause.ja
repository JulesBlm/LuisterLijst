    const paused = this.state.nowPlaying.paused;

    if (this.state.nowPlaying.id !== null) {
        if (paused) {
          console.log("Unpausing!"); 
          this.dispatchPlayOrPause("play");
        } else if (paused === false) {
          console.log("Pausing!");
          this.dispatchPlayOrPause("pause");     
        }
    } else {
        console.log("Nothing started so cant pause")
    }